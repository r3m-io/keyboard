{
  "Node": {
    "#class": "Keyboard",
    "type": "object",
    "property": [
      {
        "name": "url",
        "type": "string"
      },
      {
        "name": "command",
        "type": "string"
      },
      {
        "name": "function",
        "type": "array"
      },
      {
        "name": "uuid",
        "type": "uuid"
      },
      {
        "name": "#class",
        "type": "string"
      }
    ]
  },
  "sort": [
    "url, command"
  ],
  "is.unique": [
    "url, command"
  ],
  "sync" : {
    "interval": 60
  }
}
